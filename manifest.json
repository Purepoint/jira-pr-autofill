{
  "manifest_version": 2,
  "name": "JIRA PR Autofill",
  "permissions": [
    "declarativeContent",
    "https://github.com/ConnectedHomes/hivewebshop/compare/*",
    "https://jira.bgchtest.info/browse/*"
  ],
  "version": "0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/ConnectedHomes/hivewebshop/*"],
      "js": ["content.js"]
    }
  ],
  "page_action": {},
  "commands": {
    "all": {
      "suggested_key": {
        "default": "Alt+A"
      },
      "description": "Autofill title and description"
    },
    "link": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Autofill description"
    }
  }
}
